.navbar.navbar-inverse.navbar-fixed-top
  .container
    .navbar-header

      button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar

      a.navbar-brand(href='/home')
        span.ion-cube
        | <b>Synergy Space</b>

    .collapse.navbar-collapse
      ul.nav.navbar-nav
        li(class=title=='Home'?'active':undefined)
          a(href='/home') <b>Home</b>
          //- .li(class=title=='Rentals'?'active':undefined)
          //- a(href='/ren.tals') <b>Rentals</b>
           li.dropdown
        ul.nav.navbar-nav.navbar-left
          li.dropdown
            a.dropdown-toggle(href="#" data-toggle="dropdown") <b>Rentals</b> 
              b.caret
              ul.dropdown-menu
                li
                  a(href="/addRental") Add Rental
                li
                  a(href="/manageRentals") Manage Rentals
                li
                  a(href="/topRentals") Top Rentals


        form.navbar-form.navbar-left(role="search")
          .form-group
            input.form-control(type="text" placeholder="Search")
          button.btn.btn-default Submit
          

        ul.nav.navbar-nav.navbar-right
          li(class=title=='Settings'?'active':undefined)
            a(href='/settings') <b>Settings</b>
          li(class=title=='Profile'?'active':undefined)
            a(href='/') <b>Profile</b>
          li(class=title=='Network'?'active':undefined)
            a(href='/network') <b>Network</b>
          li(class=title=='Logout'?'active':undefined)
            a(href='/logout') <b>Logout</b>
